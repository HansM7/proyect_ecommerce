	<!-- jQuery -->
	<script src="js/jquery-2.0.0.min.js" type="text/javascript"></script>

	<!-- Bootstrap4 files-->
	<script src="js/bootstrap.bundle.min.js" type="text/javascript"></script>
	<link href="css/bootstrap.css" rel="stylesheet" type="text/css" />

	<!-- Font awesome 5 -->
	<link href="fonts/fontawesome/css/all.min.css" type="text/css" rel="stylesheet">

	<!-- custom style -->
	<link href="css/ui.css" rel="stylesheet" type="text/css" />
	<link href="css/responsive.css" rel="stylesheet" media="only screen and (max-width: 1200px)" />

	<!-- custom javascript -->
	<script src="js/script.js" type="text/javascript"></script>

	<script type="text/javascript">
		/// some script
	</script>

	</head>

	<body>


	<%- include('includes/header.ejs') %>



	<!-- ========================= SECTION CONTENT ========================= -->
	<section class="section-content padding-y">

		<!-- ============================ COMPONENT REGISTER   ================================= -->
		<div class="card mx-auto" style="max-width:520px; margin-top:40px;">
		<article class="card-body">
			<header class="mb-4">
			<h4 class="card-title">Registro de usuario</h4>
			</header>
			<form action="/user"  enctype="multipart/form-data" method="POST">
				<div class="form-row">
					<div class="col form-group">
					<label>Nombres</label>
					<input type="text" class="form-control" placeholder="" name="name" value="">
					</div> 
					<div class="col form-group">
					<label>Apellidos</label>
					<input type="text" class="form-control" placeholder="" name="lastname" value="">
					</div> 
				</div>
				<div class="form-group">
					<label>Correo</label>
					<input type="email" class="form-control" placeholder="" name="email" value="">
				</div> 
				<div class="form-group">
					<label>Direccion</label>
					<input type="text" class="form-control" placeholder="" name="direction" value="">
				</div> 

				<div class="form-row">
					<div class="form-group col-md-6">
						<label>Edad</label>
						<input class="form-control" type="number" name="age" value="">
					</div> 
					<div class="form-group col-md-6">
						<label>Foto</label>
						<input class="form-control" type="file" name="file_image">
					</div> 
				</div>
			
				<div class="form-row">
					<div class="form-group col-md-6">
						<label>País</label>
						<select id="inputState" class="form-control" name="postal_code">
							<option value="+51" >PERU (+51)</option>
						</select>
					</div> 
					<div class="form-group col-md-6">
						<label>Número de celular</label>
						<input type="text" class="form-control" name="phone_number" value="">
					</div> 
				</div> 
				<div class="form-row">
					<div class="form-group col-md-6">
						<label>Crear contraseña</label>
						<input class="form-control" type="password" name="password" value="">
					</div> 
					<div class="form-group col-md-6">
						<label>Confirmar contraseña</label>
						<input class="form-control" type="password" name="password_confirm" value=""> 
					</div> 
				</div>
				<div class="form-group">
					<button type="submit" class="btn btn-primary btn-block"> Registrar </button>
				</div> 

				

			</form>
		</article><!-- card-body.// -->
		</div> <!-- card .// -->
		<p class="text-center mt-4">Have an account? <a href="">Log In</a></p>
		<br><br>
		<!-- ============================ COMPONENT REGISTER  END.// ================================= -->


	</section>
	<!-- ========================= SECTION CONTENT END// ========================= -->


	<!-- ========================= FOOTER ========================= -->
	<footer class="section-footer border-top padding-y">
		<div class="container">
		<p class="float-md-right">
